{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "status",
        "manifest_path",
        "builtin_count",
        "preserved_count",
        "total_count",
        "dry_run",
        "summary"
      ],
      "properties": {
        "status": { "type": "string", "enum": ["pass"] },
        "manifest_path": { "type": "string" },
        "builtin_count": { "type": "integer", "minimum": 0 },
        "preserved_count": { "type": "integer", "minimum": 0 },
        "total_count": { "type": "integer", "minimum": 0 },
        "dry_run": { "type": "boolean" },
        "summary": { "type": "string" }
      },
      "additionalProperties": true
    },
    {
      "type": "object",
      "required": ["status", "manifest_path", "message"],
      "properties": {
        "status": { "type": "string", "enum": ["error"] },
        "manifest_path": { "type": "string" },
        "message": { "type": "string" },
        "usage": { "type": "string" },
        "suggestion": { "type": "string" }
      },
      "additionalProperties": true
    }
  ]
}
