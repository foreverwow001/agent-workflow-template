        "message": { "type": "string" },
        "usage": { "type": "string" },
        "suggestion": { "type": "string" }
      },
      "additionalProperties": true
    },
    {
      "type": "object",
      "required": ["status", "log_path", "statistics", "message", "usage"],
      "properties": {
        "status": { "type": "string", "enum": ["error"] },
        "log_path": { "type": "string" },
        "statistics": { "type": "object" },
        "message": { "type": "string" },
        "usage": { "type": "string" },
        "suggestion": { "type": "string" }
